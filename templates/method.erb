<%     if m[:args].count == 1 -%>
<%       if m[:args][0][0] =~/^_/ -%>
           private "<%= m[:args][0][0] -%>";
<%       end -%>
           <%- rval = "_pls_fix_macro_#{SecureRandom.hex}" -%>
           <%- if m[:args][0][1] != "nil" -%>
           private "<%= rval -%>";
           <%= rval -%> = <%= m[:args][0][1] %>
           <%- else -%>
           <%- rval = "nil" -%>
           <%- end -%>
           <%= m[:args][0][0] -%> = RPARAM(<%= rval -%>);
<%     else -%>
<%       m[:args].each_with_index do |a,i| -%>
<%         if a[0] =~/^_/ -%>
           private "<%= a[0] -%>";
           <%- end -%>
           <%- rval = "_pls_fix_macro_#{SecureRandom.hex}" -%>
           <%- if m[:args][0][1] != "nil" -%>
           private "<%= rval -%>";
           <%= rval -%> = <%= a[1] %>
           <%- else -%>
           <%- rval = "nil" -%>
           <%- end -%>
           <%= a[0] -%> = RPARAMS(<%= i -%>,<%= a[1] -%>);
<%       end -%>
<%     end -%>
<%     m[:body].each do |b| -%>
           <%= b.chomp %>
<%     end -%>
