<% @modules.each do |c| -%>
<%   c.meths.each do |m| -%>
<%= c -%>_fnc_<%= m[:name] -%> = {
<%     if m[:args].count == 1 -%>
<%       if m[:args][0][0] =~/^_/ -%>
    private "<%= m[:args][0][0] -%>";
<%       end -%>
    <%= m[:args][0][0] -%> = RPARAM(<%= m[:args][0][1] -%>);
<%     else -%>
<%       m[:args].each_with_index do |a,i| -%>
<%         if a[0] =~/^_/ -%>
    private "<%= a[0] -%>";
    <%= a[0] -%> = RPARAMS(<%= i -%>, <%= a[1] -%>);
<%         end -%>
<%       end -%>
<%     end -%>
<%     m[:body].each do |b| -%>
    <%= b.chomp %>
<%     end -%>
};
<%   end -%>
<% end %>
