<% @modules.each do |c| -%>
<%= c -%> = {
    private ["_method", "_methods", "_args"];
    _method = RPARAMS(0, "")
    _args = RPARAMS(0, [])
    <%- methss = []; c.meths.each { |m| methss << '"' + m[:name] + '"' } -%>
    _methods = [ <%= methss.join(", ") -%> ];
    switch(_method) do {
<%   c.meths.each do |m| -%>
        case "<%= m[:name] -%>": {
            private ["_this"];
            _this = _args;
<%= ERB.new(load_template("method"), nil, "-",eoutvar='_sub09').result(binding) -%>
        };
<%   end -%>
        default {
            if("method_missing" in _methods) then {
                ["method_missing", [_method, _args]] call <%= c -%>;
            };
            // method not found handling
        };
    };
};
<% end %>
